{
	"name": "classplus-instalearn-microservice",
	"version": "1.0.0",
	"description": "Instalearn APP for Content creators to monetize their calender slots via meetings",
	"main": "dist/server.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/tvvignesh/node-skeleton.git"
	},
	"jest": {
		"testEnvironment": "node",
		"timers": "fake",
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/src/*"
		],
		"testPathIgnorePatterns": [
			"src"
		],
		"collectCoverage": true,
		"coverageReporters": [
			"json",
			"html"
		],
		"setupFiles": [
			"./jest-preload-env.js"
		]
	},
	"scripts": {
		"dev": "npm run build && node ./dist/server.js",
		"build": "copyfiles -u 1 \"./src/**/*.{css,html,ico,png,svg}\" dist/ && npx tsc && npx copyfiles -u 1 -e \"src/**/*.ts\" \"src/**/*.*\" dist/",
		"watch": "tsc -w -p ./",
		"debug": "npx cross-env NODE_ENV=development nodemon --nolazy ./dist/server.js",
		"docker-debug": "npm run build && docker-compose up",
		"lint": "npx eslint --ext=.ts .",
		"document": "npx typedoc",
		"format": "prettier --write '**/*.{ts,js,css,md}'",
		"start": "node ./dist/server.js",
		"start-dev": "npm run format && npm run lint && npx cross-env NODE_ENV=development nodemon ./dist/server.js",
		"start-alpha": "npm run build && npm run lint && npx cross-env NODE_ENV=alpha node ./dist/server.js",
		"start-secure": "npm run build && npm run lint && npx cross-env NODE_ENV=secure node ./dist/server.js",
		"coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test",
		"sonar": "node sonar-project.js",
		"test": "cross-env npm run build  && rm -rf database.sqlite3 && NODE_ENV=test npx sequelize-cli db:migrate && NODE_ENV=test npx sequelize-cli db:seed:all && NODE_ENV=test npx jest --silent --runInBand dist/tests/*/*.test.js --forceExit",
		"dev-test": "npm run build && NODE_ENV=test npx jest dist/tests/user/user.details.test.js"
	},
	"author": "Classplus",
	"license": "MIT",
	"dependencies": {
		"@google-cloud/pubsub": "^2.19.4",
		"@google-cloud/storage": "^6.5.2",
		"@instana/collector": "^1.127.0",
		"@types/compression": "^1.7.2",
		"@types/jest": "^26.0.23",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/ms": "^0.7.31",
		"@types/uuid": "^8.3.0",
		"axios": "^0.21.1",
		"bluebird": "^3.7.2",
		"body-parser": "^1.19.0",
		"child_process": "^1.0.2",
		"cockatiel": "^2.0.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"cron": "^2.0.0",
		"crypto": "^1.0.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-status-monitor": "^1.3.3",
		"glob": "^7.1.6",
		"google-play-scraper": "^8.0.3",
		"helmet": "^4.4.1",
		"input": "^1.0.1",
		"iplocation": "^7.2.0",
		"jaeger-client": "^3.18.1",
		"json2csv": "^5.0.7",
		"mailgun.js": "^8.0.2",
		"method-override": "^3.0.0",
		"moment": "^2.29.4",
		"moment-timezone": "^0.5.38",
		"multer": "^1.4.5-lts.1",
		"mustache-express": "^1.3.0",
		"mysql2": "^2.2.5",
		"node-cache": "^5.1.2",
		"number-to-words": "^1.2.4",
		"nyc": "^15.1.0",
		"opentracing": "^0.14.5",
		"os": "^0.1.1",
		"passport": "^0.6.0",
		"pemtools": "^0.4.7",
		"pidusage": "^2.0.21",
		"razorpay": "^2.8.1",
		"redis": "^4.2.0",
		"reflect-metadata": "^0.1.13",
		"request": "^2.88.2",
		"request-promise": "^4.2.6",
		"sequelize": "6.19.0",
		"sequelize-typescript": "^2.1.3",
		"sonarqube-scanner": "^2.8.2",
		"swagger-ui-express": "^4.1.7",
		"telegraf": "^4.10.0",
		"telegram": "^2.12.8",
		"tinyurl": "^1.1.7",
		"ts-node": "^9.1.1",
		"underscore": "^1.13.1",
		"uuid": "^8.3.2",
		"winston": "^3.3.3",
		"xss-clean": "^0.1.1",
		"yamljs": "^0.3.0"
	},
	"ava": {
		"files": [
			"dist/tests/*.ts"
		],
		"extensions": [
			"ts"
		],
		"failWithoutAssertions": false
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.36",
		"@types/body-parser": "^1.19.0",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.11",
		"@types/express-serve-static-core": "^4.17.19",
		"@types/node": "^14.18.32",
		"@types/readable-stream": "2.3.5",
		"@types/underscore": "^1.11.4",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"acorn": "^8.1.0",
		"ava": "3.15.0",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"eslint": "^7.23.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"googleapis": "^110.0.0",
		"jest": "^26.6.3",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.7",
		"prettier": "^2.2.1",
		"sequelize-cli": "^6.2.0",
		"supertest": "^6.1.3",
		"typedoc": "^0.20.35",
		"typescript": "^4.2.3"
	}
}
